{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "linebreak-style": "off",
        "react/jsx-indent": "off",
        "indent":"off",
        "react/jsx-indent-props": "off",
        "no-console": "off",
        "react/forbid-prop-types": "off",
        "react/no-array-index-key" : "off",
        "max-len": "off"
    }
}
